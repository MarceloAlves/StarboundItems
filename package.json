{
  "name": "starbound-items",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://starbounditems.com",
  "dependencies": {
    "@sentry/cli": "^1.35.5",
    "axios": "^0.18.0",
    "bulma": "^0.7.1",
    "classnames": "^2.2.6",
    "diff-json": "^0.1.11",
    "lodash": "^4.17.10",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "raven-js": "^3.26.4",
    "react": "^16.4.2",
    "react-adsense": "^0.0.6",
    "react-debounce-input": "^3.2.0",
    "react-dom": "^16.4.2",
    "react-json-view": "^1.19.1",
    "react-router-dom": "^4.3.1",
    "react-router-ga": "^1.0.0",
    "react-scripts": "1.1.4",
    "react-select": "1.3.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "VERSION=`./node_modules/.bin/sentry-cli releases propose-version` npm-run-all build-start build-css build-js build-finalize && cp _redirects build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-start": "./node_modules/.bin/sentry-cli releases new \"$VERSION\"",
    "build-finalize": "./node_modules/.bin/sentry-cli releases finalize \"$VERSION\""
  },
  "devDependencies": {
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "nock": "^9.6.1",
    "prettier": "^1.14.2",
    "react-test-renderer": "^16.4.2",
    "sinon": "^6.1.5"
  }
}
